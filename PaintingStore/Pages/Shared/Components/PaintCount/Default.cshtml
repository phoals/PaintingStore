@using PaintingStore.Models;

@model IEnumerable<GenrePaintingCount>

    <table class="table table-hover rounded-pill">
        <thead class="thead-light">
            <tr>
                <th>Genre</th>
                <th>Paintings Count</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Genre</td>
                    <td>@item.Count</td>
                </tr>
            }
        </tbody>
    </table>